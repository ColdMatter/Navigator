<UserControl x:Class="MOTMaster2.ExtDevices.Factor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="65" d:DesignWidth="300">
    <Grid x:Name="gridCanvas">
        <Label x:Name="lbFactor" Content="Label" HorizontalAlignment="Left" Margin="20,3,0,0" VerticalAlignment="Top" FontSize="13"/>
        <ComboBox x:Name="cbFactor" Margin="20,32,20,0" VerticalAlignment="Top" Background="{x:Null}" FontSize="13" IsEditable="True" Text="Text"/>
    </Grid>
</UserControl>
