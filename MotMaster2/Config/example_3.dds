[script]
dds 0 reset
dcp 0 spi:CFR2=0x$fct1$
dcp 0 spi:STP0=0x$fct2$
dcp 0 wait::$select-bnc.trigger$
dcp 0 update:u
dcp 0 spi:STP0=0x3fff0000$fct3$
dcp 0 wait:$fct4$:
dcp 0 update:u

dcp 0 spi:STP0=0x83126F
dcp 0 wait:62500h:
dcp 0 spi:DRL=0x$fct5$00000000
dcp 0 spi:DRSS=0x$fct6$$fct7$
dcp 0 spi:DRR=0x1F401F4
dcp 0 spi:CFR2=0x280080
dcp 0 wait:$fct12$:
dcp 0 update:u+d

dcp 0 spi:STP0=0x$fct10$
dcp 0 wait:$fct11$:
dcp 0 update:u

dcp 0 update:-d
dcp 0 spi:CFR2=0x1200080
dcp 0 spi:STP0=0x418937
dcp 0 wait:62500h:
dcp 0 update:u
dcp 0 start

[factors]
fct1=Factor 1 [dBm]
fct2=Factor 2 [MHz]
fct3=Factor 3 [MHz]
fct4=Factor 4 [ns]
fct5=Factor 5 [MHz]
fct6=Factor 6 [Hz]
fct7=Factor 7 [Hz]
fct8=Factor 8 [MHz]
fct9=Factor 9 [ns]
fct10=Factor 10 [MHz]
fct11=Factor 11 [ns]
fct12=Factor 12 [ns]
fct13=Factor 13 [unit]
fct14=Factor 14 [unit]





