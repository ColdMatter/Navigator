<UserControl x:Class="MOTMaster2.ExtDevices.Factor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="65" d:DesignWidth="300" Height="55">
    <Grid x:Name="gridCanvas">
        <Grid.RowDefinitions>
            <RowDefinition x:Name="firstRow" Height="28"/>
            <RowDefinition x:Name="factorRow" Height="25"/>
        </Grid.RowDefinitions>

        <Label x:Name="lbFactor" Content="Name" Margin="0,0,0,0" VerticalAlignment="Top" FontSize="13" Foreground="Navy" HorizontalAlignment="Left"/>
        <Label x:Name="lbExtName" Content="ExtName" Margin="0,0,3,0" VerticalAlignment="Top" FontSize="13" Foreground="DarkGray" HorizontalAlignment="Right"/>
        <ComboBox x:Name="cbFactor" Margin="5,0,5,3" VerticalAlignment="Bottom" Background="{x:Null}" FontSize="13" IsEditable="True" Text="Text" Grid.Row="1"/>
    </Grid>
</UserControl>
