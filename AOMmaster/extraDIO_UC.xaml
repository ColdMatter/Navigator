<UserControl x:Name="ExtraDO_UC" x:Class="AOMmaster.ExtraDIO_UC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AOMmaster"
             mc:Ignorable="d" 
             d:DesignHeight="650" d:DesignWidth="263.218">
    <Grid Margin="0,0,0.089,0.228">
        <Grid.RowDefinitions>
            <RowDefinition Height="1"/>
            <RowDefinition Height="450"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <DataGrid x:Name="dataGrid" Margin="0" Grid.Row="1" RowHeight="20" ColumnWidth="35" FrozenColumnCount="1" CanUserReorderColumns="False" CanUserResizeRows="False" SelectionUnit="Cell" AlternatingRowBackground="#FFF9F9EF"/>
        <RichTextBox x:Name="richLog" Margin="0" Grid.Row="2">
            <FlowDocument>
                <Paragraph>
                    <Run Text="Digit.Output Log
                         "/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Button x:Name="btnUpdate" Content="Digit.Outputs Update" HorizontalAlignment="Center" Margin="0" VerticalAlignment="Center" Width="130" Background="#FFE8FAF3" Click="btnUpdate_Click"/>
    </Grid>
</UserControl>
